{
  "proxies": {
    "files": {
      "matchCondition": {
        "route": "/static/{*path}"
      },
      "backendUri": "https://%WEBSITE_SITE_NAME%.azurewebsites.net/api/StaticFileServer?file={path}"
    },
    "favicon": {
      "matchCondition": {
        "route": "/favicon.ico"
      },
      "backendUri": "https://%WEBSITE_SITE_NAME%.azurewebsites.net/api/StaticFileServer?file=favicon.ico"
    }
  }
}